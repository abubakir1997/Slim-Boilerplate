{
    "name": "slim-react-boilerplate",
    "description": "Slim React Boilerplate",
    "type": "project",
    "require": {
        "slim/slim": "^3.9",
        "slim/twig-view": "^2.3",
        "monolog/monolog": "^1.23",
        "slim/flash": "^0.4.0",
        "kanellov/slim-twig-flash": "^0.2.0",
        "robmorgan/phinx": "^0.9.1",
        "illuminate/database": "^5.5",
        "slim/csrf": "^0.8.2",
        "firebase/php-jwt": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Libs\\": "libs/",
            "Core\\": "core/",
            "Controllers\\": "app/Controllers/",
            "Models\\": "app/Models/",
            "Middlewares\\": "app/Middlewares/"
        }
    },
    "authors": [
        {
            "name": "Abdelrahmam Salem",
            "email": "abubakir1997@gmail.com"
        }
    ],
    "scripts": {
        "npm-build": "cd npm; npm run build; cd ../;",
        "npm-dev"  : "cd npm; npm run dev; cd ../;",
        "npm-clean": "cd npm; npm run clean; cd ../;"
    }
}
